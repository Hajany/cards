{
  "install": {
    "base": "/Vocabularies?source=bioontology",
    "params": {
      "overwrite": "true"
    }
  },
  "uninstall": {
    "base": "/Vocabularies/"
  },
  "local": "/Vocabularies.1.json",
  "recommender": {
    "base": "https://data.bioontology.org/recommender?"
  },
  "remote": {
    "base": "https://data.bioontology.org/submissions/?",
    "params": {
      "display_context": false,
      "display_links": false,
      "include" : [
        "acronym",
        "description",
        "name",
        "ontology",
        "released",
        "status",
        "version"
      ]
    }
  }
}
